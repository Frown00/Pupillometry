{
  "name": "Just testing",
  "file": {
    "separator": ",",
    "timestamp": "Timestamp",
    "leftPupil": "ET_PupilLeft",
    "rightPupil": "ET_PupilRight",
    "segmentActive": "Scene active"
  },
  "chart": {
    "width": 1000,
    "height": 500,
    "showEyesPlot": true,
    "showMeanPlot": true,
    "showSmoothed": true,
    "showRejected": ["invalid", "outliers"]
  },
  "measurement": {
    "eye": "both",
    "baseline": {
      "type": "from Start",
      "param": 1000
    },
    "segmentation": "scene",
    "windows": []
