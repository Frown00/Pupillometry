    "trendlineDeviation": {
      "on": true,
      "thresholdMultiplier": 1,
      "cutoffFrequency": 4,
      "passes": 1,
      "gapMinimumDuration": 50,
      "gapMaximumDuration": 70,
      "backwardGapPadding": 50,
      "forwardGapPadding": 50
    },
    "temporallyIsolatedSamples": {
      "on": true,
      "sizeMaximum": 50,
      "isolationMinimum": 40
    }
  },
  "validity": {
    "missing": 40,
    "correlation": 0.5,
    "difference": 1
  },
  "resampling": {
    "on": false,
    "rate": 1000,
    "acceptableGap": 250
  },
  "smoothing": {
    "on": true,
    "cutoffFrequency": 4
