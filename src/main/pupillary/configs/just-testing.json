  "markers": {
    "outOfRange": {
      "min": 2,
      "max": 9
    },
    "dilatationSpeed": {
      "on": true,
      "thresholdMultiplier": 3,
      "gapMinimumDuration": 50,
      "gapMaximumDuration": 70,
      "backwardGapPadding": 50,
      "forwardGapPadding": 50
